{
  "incremental": true,
  "typescript": {
    "$schema": "https://plugins.dprint.dev/schemas/typescript-0.0.0.json"
  },
  "json": {},
  "markdown": {
    "$schema": "https://plugins.dprint.dev/schemas/markdown-0.0.0.json"
  },
  "includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json,md}"],
  "excludes": ["**/node_modules", "**/*-lock.json"],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.59.2.wasm",
    "https://plugins.dprint.dev/json-0.13.2.wasm",
    "https://plugins.dprint.dev/markdown-0.11.3.wasm"
  ],
  "indentWidth": 2,
  "useTabs": false
}
