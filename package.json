    "cheerio": "^1.0.0-rc.10",
    "got": "^11.8.2",
    "iconv-lite": "^0.6.3",
    "is-url-online": "^1.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "9.0.1",
    "@semantic-release/git": "9.0.1",
    "@semantic-release/github": "8.0.1",
    "@semantic-release/npm": "8.0.0",
    "@semantic-release/release-notes-generator": "10.0.2",
    "@types/cheerio": "0.22.30",
    "@types/jest": "27.0.2",
    "@types/node": "16.7.1",
    "commitizen": "4.2.4",
    "dotenv": "10.0.0",
    "esbuild": "0.12.28",
    "esbuild-jest": "0.5.0",
    "eslint": "7.32.0",
    "eslint-config-galex": "2.16.12",
    "eslint-plugin-typescript-sort-keys": "1.8.0",
    "husky": "7.0.2",
    "jest": "27.2.1",
    "nodemon": "2.0.12",
    "prettier": "2.4.1",
    "semantic-release": "18.0.0",
    "tslib": "2.3.1",
    "tsup": "5.1.0",
    "type-fest": "2.3.3",
    "typescript": "4.4.3"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/Unbuttun/quick-scraper#readme",
  "husky": {
    "hooks": {
      "pre-commit": "pnpm format && pnpm lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "scraper",
    "decoding",
    "iconv",
    "utf-8",
    "quick",
    "spider",
    "crawler"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Unbuttun/quick-scraper.git"
  },
  "scripts": {
    "build": "NODE_ENV='production' tsup-node",
    "build:dev": "NODE_ENV='development' tsup-node",
    "cz": "cz",
    "dev": "nodemon dist/index.js",
    "format": "prettier --write .",
    "lint": "eslint src --ext .ts",
    "semantic-release": "semantic-release",
    "test": "jest"
  },
  "typings": "dist/index.d.ts"
}
